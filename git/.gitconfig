[user]
	name = "Benjamin Heng"
	email = "ben@hbenjamin.com"
[pager]
	diff = diff-so-fancy | less
[alias]
    fix-conflicts = !git diff --name-only | uniq | xargs nvim
    logfzf = !git-fzflog
    checkoutfzf = !git checkout $(git branch --sort=-committerdate | fzf)
[url "git@github.com:"]
	insteadOf = https://github.com/
